{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/lumen-framework": "5.5.*",
        "vlucas/phpdotenv": "~2.2",
        "zaininnari/html-minifier": "*",
        "barryvdh/laravel-cors": "0.11.0",
        "wn/lumen-generators": "^1.3",
        "illuminate/session": "^5.5",
        "illuminate/cookie": "^5.5",
        "nordsoftware/lumen-cors": "^2.2",
        "palanik/lumen-cors": "dev-master",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/mail": "^5.5",
        "kelvinmo/fernet-php": "^0.5.0",
        "glenscott/url-normalizer": "^1.4",
        "illuminate/container": "^5.5",
        "zizaco/entrust": "^1.9",
        "irazasyed/larasupport": "~1.0",
        "overtrue/laravel-lang": "~3.0",
        "itsgoingd/clockwork": "~1.12",
        "tacnoman/dephpugger": "^1.1",
        "prismatix/prismatix-sdk-php": "dev-master"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~6.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "platform": {
      "php": "7.2.3"
    },
    "repositories": [
      {
        "type": "vcs",
        "url": "https://xenophy.info:9090/xenophy/prismatix-sdk-php.git"
      }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "optimize-autoloader": true
}
